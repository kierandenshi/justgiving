{
  "name": "justgiving",
  "version": "1.0.0",
  "description": "Just Giving progammer test",
  "main": "index.js",
  "author": "Kieran Denshi",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env DEPLOYMENT_ENV=local NODE_ENV=development webpack-dev-server --progress --colors --hot",
    "build:clean": "rm -rf dist && mkdir dist",
    "build:copy": "cp ./src/index.html ./dist/ && cp -R ./src/static ./dist/static",
    "build:js": "cross-env NODE_ENV=production webpack -p --config ./webpack.config.js --progress --profile --colors",
    "build:all": "npm run build:clean && npm run build:copy && npm run build:js",
    "build": "npm run build:all",
    "eslint": "eslint ./**/*.js",
    "test": "cross-env NODE_ENV=test mocha ./src/app --require ./tests/test-init --compilers js:babel-register --recursive"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "chai": "^4.1.2",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-react": "^7.9.1",
    "jsdom": "^11.11.0",
    "match-media-mock": "^0.1.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.5.0",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "4.3.1",
    "sass-loader": "^7.0.3",
    "sinon": "^5.1.1",
    "sinon-chai": "^3.1.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.0",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "cross-env": "^5.1.6",
    "extract-text-webpack-plugin": "^3.0.2",
    "flexibility": "^2.0.1",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.3",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1"
  }
}
