{
  "name": "justgiving",
  "version": "1.0.0",
  "description": "Just Giving progammer test",
  "main": "index.js",
  "author": "Kieran Denshi",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env DEPLOYMENT_ENV=local NODE_ENV=development webpack-dev-server --progress --colors --hot",
    "build:clean": "rm -rf dist && mkdir dist",
    "build:copy": "cp ./src/index.html ./dist/ && cp -R ./src/static ./dist/static",
    "build:js": "cross-env NODE_ENV=production webpack -p --config ./webpack.config.js --progress --profile --colors",
    "build:all": "npm run build:clean && npm run build:copy && npm run build:js",
    "build": "npm run build:all",
    "eslint": "eslint ./**/*.js",
    "test": "cross-env NODE_ENV=test mocha ./src/app --require ./tests/test-init --compilers js:babel-register --recursive"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-module-resolver": "^2.5.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.2",
    "eslint": "^3.16.1",
    "eslint-plugin-babel": "^4.1.0",
    "eslint-plugin-react": "^6.10.0",
    "jsdom": "^9.11.0",
    "match-media-mock": "^0.1.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "sass-loader": "^6.0.2",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "cross-env": "^3.1.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "flexibility": "^2.0.1",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-saga": "^0.14.3",
    "reselect": "^2.5.4"
  }
}
